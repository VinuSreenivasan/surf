outputfile=../experiments//exp-01/results/00002.dat

echo INPUT:['#pragma omp parallel for private (pidx) schedule(static)', '#pragma omp parallel for private (pidx) schedule(static)'] > ../experiments//exp-01/results/00002.dat
clang -fopenmp -lm ../experiments//exp-01/tmp_files/00002.c -o ../experiments//exp-01/tmp_files/00002
st=$(date '+%s')
echo START TIME:$st >> ../experiments//exp-01/results/00002.dat
./../experiments//exp-01/tmp_files/00002 16 1 16 400 32 1 100
et=$(date '+%s')
echo END TIME:$et>> ../experiments//exp-01/results/00002.dat
run=$((et-st))
echo OUTPUT:$run >> ../experiments//exp-01/results/00002.dat
echo


