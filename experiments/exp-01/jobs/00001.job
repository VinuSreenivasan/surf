#!/bin/bash -x
#COBALT -n 1
#COBALT -q debug-flat-quad
#COBALT -A Performance
#COBALT -t 30

outputfile=../experiments//exp-01/results/00001.dat

echo INPUT:['#pragma omp parallel for private (pidx) schedule(dynamic)', '#pragma omp parallel for private (pidx) schedule(static)'] > ../experiments//exp-01/results/00001.dat
clang -fopenmp -lm ../experiments//exp-01/tmp_files/00001.c -o ../experiments//exp-01/tmp_files/00001
st=$(date '+%s')
echo START TIME:$st >> ../experiments//exp-01/results/00001.dat
./../experiments//exp-01/tmp_files/00001 16 1 16 400 32 1 100
et=$(date '+%s')
echo END TIME:$et>> ../experiments//exp-01/results/00001.dat
run=$((et-st))
echo OUTPUT:$run >> ../experiments//exp-01/results/00001.dat
echo


